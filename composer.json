{
  "name": "industrialdev/wicket-wp-base-plugin",
  "description": "Wicket Core Plugin",
  "version": "2.1.42",
  "keywords": ["wordpress", "plugin"],
  "homepage": "https://github.com/industrialdev/wicket-wp-base-plugin",
  "authors": [
    {
      "name": "Wicket",
      "homepage": "https://wicket.io"
    }
  ],
  "type": "wordpress-plugin",
  "require": {
    "industrialdev/wicket-sdk-php": "dev-master",
    "php": ">=8.0",
    "jeffreyvanrossum/wp-settings": "^1"
  },
  "autoload": {
    "psr-4": {
      "WicketWP\\": "src/"
    }
  },
  "scripts": {
    "version-bump": [
      "@php ./.ci/version-bump.php"
    ],
    "dump-autoload-prod": "@php composer dump-autoload --optimize --no-dev"
  },
  "repositories": [
  {
    "type": "git",
    "url": "https://github.com/industrialdev/wicket-sdk-php.git"
  }
 ]
}
